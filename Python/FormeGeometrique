class FormeGeometrique:
    """
    Classe abstraite FormeGeometrique
    """
    # Attributs statiques
    nbrInstances = 0

    def __init__(self, nom:str) -> None:
        """Constructeur FormeGeometrique
        Incremente nbrInstances et affecte le nom

        Args:
            nom (str): nom de la forme
        """
        FormeGeometrique.nbrInstances += 1
        self.identifiant = nom + "_" + str(FormeGeometrique.nbrInstances)

    def calculer_surface(self) -> int:
        """Retourne la surface en mm

        Returns:
            int: Retourne la surface en mm
        """

    def  __eq__(self, other: object) -> bool:
        return other != None and self.__class__ == other.__class__ and 

    def __str__(self) -> str:
        """Redefinition de __str__

        Returns:
            str: chaine de caractere formatee non complete
        """
        return "\nForme : " + self.identifiant + "\nDe type : " + self.__class__.__name__ + "\nSurface : " + str(self.calculer_surface());
